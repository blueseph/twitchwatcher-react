{
    "parser": "babel-eslint",
    "rules": {
      "strict": 0
    },
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jasmine": true
    },
    "plugins": [
        "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
}
